<!DOCTYPE HTML>
<html>
<head>
   @await Html.PartialAsync("MetatagsPartial")
   @await Html.PartialAsync("CssPartial")
</head>
<body>
    <div id="page-wrapper">

        <!-- Header -->
        @await Html.PartialAsync("HeaderPartial")
        <!-- Main -->
        <div id="main">
            <div class="container">
                <div class="row main-row">
                    <div class="col-16 col-32-medium">
                        @RenderBody()
                    </div>
                </div>
                @await Html.PartialAsync("SiderbarPartial")
            </div>
        </div>

        <!-- Footer -->
        @await Html.PartialAsync("FooterPartial")

    </div>

    <!-- Scripts -->
    @await Html.PartialAsync("ScriptsPartial")
</body>
</html>
